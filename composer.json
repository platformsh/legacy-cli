{
    "name": "platformsh/cli",
    "description": "Platform.sh CLI",
    "license": "MIT",
    "require": {
        "cocur/slugify": "~1.0",
        "commerceguys/guzzle-5-oauth2-plugin": "dev-guzzle5-support",
        "symfony/console": "~2.5",
        "symfony/yaml": "~2.5",
        "symfony/finder": "~2.5",
        "symfony/filesystem": "~2.5",
        "symfony/options-resolver": "~2.5",
        "symfony/process": "~2.5",
        "stecman/symfony-console-completion": "0.4.4",
        "guzzlehttp/guzzle-services": "~0.4.0"
    },
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/pjcdawkins/guzzle-oauth2-plugin.git"
        }
    ],
    "suggest": {
        "drush/drush": "For Drupal projects"
    },
    "autoload": {
        "psr-4": {
            "CommerceGuys\\Platform\\Cli\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "mikey179/vfsStream": "~1.4"
    },
    "authors": [
        {
            "name": "Bojan Zivanovic"
        }
    ],
    "bin": [
        "platform"
    ]
